provider "aws" {
  region = "us-east-1"  # Change to your preferred AWS region
}

resource "aws_instance" "my_ec2" {
  ami           = "ami-0c55b159cbfafe1f0"  # Ubuntu AMI (Check AWS for latest)
  instance_type = "t2.micro"
  key_name      = "My_File_Storage"  # Ensure this key pair exists in AWS
  tags = {
    Name = "Terraform-Instance"
  }
}
